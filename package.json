{
  "name": "cweepy-es6",
  "author": "Simon Hojberg",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node server",
    "compile": "browserify -v -t [ babelify ] app/main.jsx -o public/js/bundle.js",
    "watch": "watchify -v -t [ babelify ] app/main.jsx -o public/js/bundle.js",
    "develop": "concurrently --kill-others \"npm run watch\" \"nodemon server.js\""
  },
  "dependencies": {
    "antidote_ts_client": "0.0.2",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "babelify": "^7.3.0",
    "express": "3.0.0",
    "js-cookie": "^2.1.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "browserify": "13.1.0",
    "concurrently": "^3.1.0",
    "nodemon": "^1.11.0",
    "watchify": "3.7.0"
  },
  "engine": {
    "node": ">=0.8.x"
  }
}
